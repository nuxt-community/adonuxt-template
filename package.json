    "adonis-framework": "^3.0.9",
    "adonis-lucid": "^3.0.13",
    "adonis-middleware": "^1.0.10",
    "adonis-websocket": "^1.0.2",
    "cross-env": "^5.0.4",
    "nuxt": "latest",
    "standard": "^10.0.3",
    "youch": "^2.0.4"
  },
  "standard": {
    "global": [
      "use",
      "make"
    ]
  },
  "autoload": {
    "App": "./app"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-promise": "^3.4.0",
