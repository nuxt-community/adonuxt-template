    "@adonisjs/lucid": "^5.0.3",
    "@adonisjs/session": "^1.0.19",
    "@adonisjs/shield": "^1.0.4",
    "cross-env": "^5.1.4",
    "nuxt": "latest",
    "standard": "^11.0.1",
    "youch": "^2.0.4"
